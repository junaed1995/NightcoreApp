<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/luffy"

    android:soundEffectsEnabled="false"
    android:visibility="visible"
    tools:context=".About_me">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="159dp"
        android:layout_height="63dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/aclonica"
        android:text="About Me:"
        android:textAlignment="center"
        android:textColor="#FFFF00"
        android:textSize="28dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mutlitext"
        android:layout_width="268dp"
        android:layout_height="263dp"
        android:layout_marginTop="40dp"
        android:fontFamily="cursive"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#FFFF00"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/bttn"
        android:layout_width="134dp"
        android:layout_height="55dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/chewy"
        android:text="Back"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/clickable" />

    <TextView
        android:id="@+id/clickable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="172dp"
        android:layout_marginEnd="180dp"
        android:layout_marginBottom="115dp"
        android:autoLink="web"
        android:clickable="true"
        android:fontFamily="@font/boogaloo"
        android:text="Link"
        android:textAlignment="center"
        android:textColor="#FFFF00"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/bttn"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mutlitext" />


</androidx.constraintlayout.widget.ConstraintLayout>